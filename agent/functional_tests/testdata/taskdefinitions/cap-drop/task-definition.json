{
  "family": "ecsftest-cap-drop",
  "containerDefinitions": [{
    "image": "127.0.0.1:51670/ubuntu:latest",
    "name": "exit",
    "cpu": 10,
    "memory": 10,
    "linuxParameters": {
      "capabilities": {
        "drop": ["MKNOD"]
      }
    },
    "command": ["sh", "-c", "capsh --print | grep \"cap_mknod\" && exit 1 || exit 42"]
  }]
}
