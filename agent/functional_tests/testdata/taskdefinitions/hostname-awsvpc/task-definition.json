{
"family": "hostname-awsvpc",
"networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "exit",
        "image": "127.0.0.1:51670/ubuntu:latest",
        "cpu": 100,
        "command":  [ "sh", "-c", "hostname | grep \"ip-[0-9]*-[0-9]*-[0-9]*-[0-9]*.*.compute.internal\" && exit 0 || exit 1" ],
        "memory": 100,
        "essential": true
      }]
}
