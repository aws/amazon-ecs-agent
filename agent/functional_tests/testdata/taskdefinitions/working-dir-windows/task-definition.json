{
  "family": "ecsftest-working-dir-windows",
  "containerDefinitions": [{
    "image": "microsoft/windowsservercore:latest",
    "name": "exit",
    "cpu": 1024,
    "memory": 512,
    "workingDirectory": "C:/windows/system32",
    "command": ["powershell", "-c", "if ((pwd).Path -eq \"C:\\windows\\system32\") { exit 42 } ; exit 1"]
  }]
}
