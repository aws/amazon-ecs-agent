{
  "family": "ecsftest-cap-add",
  "containerDefinitions": [{
    "image": "127.0.0.1:51670/ubuntu:latest",
    "name": "exit",
    "cpu": 10,
    "memory": 10,
    "linuxParameters": {
      "capabilities": {
        "add": ["SYS_ADMIN"]
      }
    },
    "command": ["sh", "-c", "capsh --print | grep \"cap_sys_admin\" && exit 42 || exit 1"]
  }]
}
